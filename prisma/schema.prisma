// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Event {
  id         String   @id @default(uuid())
  name       String
  date       DateTime
  address    Json
  type       String
  created_at DateTime @default(now())

  @@map("event")
}
